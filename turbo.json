{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "dev": {
            "persistent": true
        },
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "!.next/cache/**",
                ".next/**",
                "$TURBO_ROOT$/target/**",
                "dist/**",
                "pkg/**",
                "target/**"
            ]
        }
    }
}
